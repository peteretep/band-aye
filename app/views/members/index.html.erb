<h1>Members</h1>

<table class="table">
<tr>
  <th>Email</th>
  <th>Name</th>
  <th>Instrument</th>
  <th> </th>
<% @members.each do |member| %>
  <tr>
    <td><%= member.email %></td>
    <td><%= member.full_name %></td>
    <td><%= member.instrument_list.to_sentence %></td>
    <td><%= member.admin_role %></td>
<% end %>

</table>